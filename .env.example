# BioDockify Environment Variables Example
# Copy this file to .env and fill in your keys

# AI Providers
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# LLM Keys
GOOGLE_API_KEY=
OPENROUTER_API_KEY=
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Database
DATABASE_URL=postgresql://biodockify:biodockify@db:5432/biodockify
REDIS_URL=redis://redis:6379/0

# Vector DB
VECTOR_DB_TYPE=chroma
VECTOR_DB_PATH=/app/data/chroma
MEMORY_PERSIST_DIR=/app/data/chroma_memory

# Monitoring
ENABLE_MONITORING=true
SENTRY_DSN=

# Security
BIODOCKIFY_EMERGENCY_SECRET=your-random-secret-here
ENVIRONMENT=development
